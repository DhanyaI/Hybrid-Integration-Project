	<munit:test name="sa0004-galaxy-test-suite-sa0004-galaxy-mainTest-Success" doc:id="b9be4272-0349-4c4d-9c10-3aaeb809950b" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock API Router Kit" doc:id="cb0dd683-b6d1-4546-939f-347fdd702a5c" processor="apikit:router">
				<munit-tools:with-attributes >
