					</on-error-continue>
				</error-handler>
			</try>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert doc:name="Assert expression" doc:id="df7d2263-1a6f-4a95-aea3-c4f605117169" />
		</munit:validation>
	</munit:test>
