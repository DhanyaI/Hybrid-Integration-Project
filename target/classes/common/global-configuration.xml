	<configuration-properties doc:name="common-properties-configuration" doc:id="1cc9b5a1-5a24-418c-abe4-80c7dfe35e2c" file="config\config-common.yaml" />
	<configuration-properties doc:name="environment-properties-configuration" doc:id="cb7fe809-5412-45fa-868f-e0ca2a63122e" file="config\config-${mule.env}.yaml" />
    <apikit:config name="sa0004-galaxy-config" api="resource::4fca20b5-fe60-4964-8bbc-b266b54be3b7:sa0001-galaxy:0.1.0:raml:zip:sa0004-galaxy.raml" outboundHeadersMapName="outboundHeaders" httpStatusVarName="httpStatus" />
